{
  "expo": {
    "name": "Diner",
    "slug": "diner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "diner",
    "splash": {
      "image": "./assets/Splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#1B002C"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sbhagwat.diner"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1B002C"
      },
      "package": "com.sbhagwat.diner"
    },
    "androidStatusBar": {
      "translucent": true,
      "barStyle": "light-content"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
